<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="@color/white"/>
            <size
                android:height="2dp"
                android:width="2dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/FS_ORANGE"/>
        </shape>
    </item>

    <item
        android:drawable="@drawable/sun"
        android:top="2dp"
        android:bottom="2dp"
        android:right="2dp"
        android:left="2dp"/>

</layer-list>